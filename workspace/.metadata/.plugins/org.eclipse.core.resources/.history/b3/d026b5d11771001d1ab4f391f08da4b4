package OOP;

class Marin
{
	public int hp = 100;
	public void run()
	{
		hp -= 10;
		System.out.println("running ... ");
	}
	public Marin(int hp)
	{
		this.hp = hp;
	}
}

public class Main {

	public static void main(String[] args) {
		// 생성자 이용해서 값 초기화
		Marin m1 = new Marin(100);
		m1.run();
	}

}
